{
  "name": "@node-rs/argon2",
  "version": "1.5.0",
  "description": "RustCrypto: Argon2 binding for Node.js",
  "homepage": "https://github.com/napi-rs/node-rs",
  "main": "index.js",
  "types": "index.d.ts",
  "keywords": [
    "argon2",
    "argon2-password",
    "crypto",
    "password",
    "encryption",
    "hashing",
    "hash",
    "secure",
    "verify"
  ],
  "napi": {
    "name": "argon2",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-freebsd",
        "i686-pc-windows-msvc",
        "armv7-linux-androideabi"
      ]
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/napi-rs/node-rs.git"
  },
  "scripts": {
    "artifacts": "napi artifacts -d ../../artifacts",
    "bench": "cross-env NODE_ENV=production node benchmark/argon2.js",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish",
    "version": "napi version"
  },
  "devDependencies": {
    "argon2": "^0.30.3",
    "cross-env": "^7.0.3"
  },
  "gitHead": "8eeb2c75f4d17cdc31852cc295c487fc91f8fcba",
  "optionalDependencies": {
    "@node-rs/argon2-win32-x64-msvc": "1.5.0",
    "@node-rs/argon2-darwin-x64": "1.5.0",
    "@node-rs/argon2-linux-x64-gnu": "1.5.0",
    "@node-rs/argon2-darwin-arm64": "1.5.0",
    "@node-rs/argon2-android-arm64": "1.5.0",
    "@node-rs/argon2-linux-arm64-gnu": "1.5.0",
    "@node-rs/argon2-linux-arm64-musl": "1.5.0",
    "@node-rs/argon2-win32-arm64-msvc": "1.5.0",
    "@node-rs/argon2-linux-arm-gnueabihf": "1.5.0",
    "@node-rs/argon2-linux-x64-musl": "1.5.0",
    "@node-rs/argon2-freebsd-x64": "1.5.0",
    "@node-rs/argon2-win32-ia32-msvc": "1.5.0",
    "@node-rs/argon2-android-arm-eabi": "1.5.0"
  }
}